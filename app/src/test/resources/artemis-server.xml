<configuration xmlns="urn:activemq" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			   xsi:schemaLocation="urn:activemq /schema/artemis-configuration.xsd">
	<core xmlns="urn:activemq:core">
		<acceptors>
			<acceptor name="invm">vm://0</acceptor>
		</acceptors>
		<security-enabled>false</security-enabled>
		<persistence-enabled>false</persistence-enabled>
		<address-settings>
			<address-setting match="qopp001">
				<dead-letter-address>qopp001Bq</dead-letter-address>
				<expiry-address>qopp001Bq</expiry-address>
				<max-delivery-attempts>1</max-delivery-attempts>
			</address-setting>
		</address-settings>

		<addresses>
			<address name="qopp001">
				<anycast>
					<queue name="qopp001"/>
				</anycast>
			</address>
			<address name="qopp001FunkFeil">
				<anycast>
					<queue name="qopp001FunkFeil"/>
				</anycast>
			</address>
			<address name="qopp001Bq">
				<anycast>
					<queue name="qopp001Bq"/>
				</anycast>
			</address>
		</addresses>
	</core>
</configuration>