<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.1.7.RELEASE</version>
	</parent>
	<groupId>no.nav.dokopp</groupId>
	<artifactId>dokopp</artifactId>
	<version>1-SNAPSHOT</version>

	<name>dokopp</name>
	<packaging>pom</packaging>

	<modules>
		<module>app</module>
	</modules>

	<distributionManagement>
		<repository>
			<id>m2internal</id>
			<name>NAV Internal Repository</name>
			<url>dav:http://maven.adeo.no/nexus/content/repositories/m2internal</url>
		</repository>
		<snapshotRepository>
			<id>m2snapshot</id>
			<name>NAV Snapshot Repository</name>
			<url>dav:http://maven.adeo.no/nexus/content/repositories/m2snapshot</url>
		</snapshotRepository>
	</distributionManagement>

	<properties>
		<start-class>no.nav.dokopp.Application</start-class>
		<project.scm.id>stash</project.scm.id>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<java.version>1.8</java.version>
		<wmq.version>9.1.3.0</wmq.version>
		<lombok.version>1.18.8</lombok.version>
		<activemq.version>5.15.0</activemq.version>
		<saxon.version>9.0</saxon.version>
		<javax.jms-api.version>1.1.1</javax.jms-api.version>
		<javax.transaction.version>1.1</javax.transaction.version>
		<javax.inject.version>1</javax.inject.version>
		<modig-log-common.version>1.1.3</modig-log-common.version>
		<nav-behandleOppgave-v1-tjenestespesifikasjon>1.0.7</nav-behandleOppgave-v1-tjenestespesifikasjon>
		<awaitility.version>3.0.0</awaitility.version>
		<commons-io.version>2.4</commons-io.version>
		<wiremock-standalone.version>2.15.0</wiremock-standalone.version>
		<xmlunit-core.version>2.3.0</xmlunit-core.version>
		<xmlunit-legacy.version>2.3.0</xmlunit-legacy.version>
		<modig-testcertificates.version>1.2.5</modig-testcertificates.version>
		<logstash-logback-encode.version>5.3</logstash-logback-encode.version>
		<!-- Se https://issues.apache.org/jira/browse/HTTPCLIENT-1997 for bug i httpclient 4.5.9 -->
		<httpclient.version>4.5.8</httpclient.version>
		<camel.version>2.24.0</camel.version>
		<resilience4j.version>0.17.0</resilience4j.version>
		<rxjava.version>2.2.9</rxjava.version>
		<!-- Take care when updating any of these. modig-security depends on CXF versions that might be incompatible. -->
		<modig-security.version>3.0.6</modig-security.version>
		<cxf.version>3.2.1</cxf.version>
	</properties>

	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
		<extensions>
			<extension>
				<groupId>org.apache.maven.wagon</groupId>
				<artifactId>wagon-webdav-jackrabbit</artifactId>
				<version>1.0</version>
			</extension>
		</extensions>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>3.0.0-M3</version>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-failsafe-plugin</artifactId>
				<version>3.0.0-M3</version>
			</plugin>
		</plugins>
	</build>
</project>